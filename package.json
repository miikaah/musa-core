{
  "name": "@miikaah/musa-core",
  "version": "7.10.3",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch ./src --ext ts --exec npm run build:w",
    "build": "shx rm -rf lib && ts-node --transpile-only esbuild.ts && npm run build:clean && tsc --emitDeclarationOnly --project tsconfig.build.json",
    "build:clean": "ts-node --transpile-only clean-build.ts",
    "build:w": "ts-node --transpile-only esbuild.ts && tsc --emitDeclarationOnly --project tsconfig.build.json --incremental",
    "tsc:w": "tsc --watch --noEmit",
    "test": "jest --coverage",
    "test:mocha": "ts-mocha",
    "test:w": "jest --watch",
    "test:heap": "jest --coverage --runInBand --logHeapUsage",
    "test:debug": "node --inspect-brk --expose-gc ./node_modules/jest/bin/jest",
    "test:debug:mocha": "node --inspect-brk --expose-gc ./node_modules/ts-mocha/bin/ts-mocha"
  },
  "author": "Miika Henttonen",
  "license": "MIT",
  "dependencies": {
    "@seald-io/nedb": "^3.1.0",
    "fuzzysort": "^2.0.3",
    "lodash.uniqby": "^4.7.0",
    "metaflac-js": "^1.0.5",
    "music-metadata": "^8.1.0",
    "node-id3": "^0.2.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@types/jest": "^29.2.3",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/mocha": "^10.0.0",
    "@types/nedb": "^1.8.12",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "esbuild": "^0.15.14",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "shx": "^0.3.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "files": [
    "lib"
  ]
}
